@using System.Web.Optimization
@using Academy.Presentation.ViewModels
@{
    //OBSOLETE
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/Internal.cshtml";
}
@section styles {
    @Styles.Render("~/Content/tree")
}
@section UserName
{
    @{ Html.RenderPartial("UserName", Model); }
}
@section Profile
{
    <div class="span3">
        @{ Html.RenderPartial("Profile", Model); }
    </div>
}

<div class="content">
    <div class="span9">
        @{ Html.RenderPartial("../Article/PublishArticle", new ArticleViewModel()); }
        @*@{ Html.RenderPartial("TestCreate", new ArticleViewModel()); }*@
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/tree")
    @Scripts.Render("~/bundles/add-author")
}