@using System.Web.Optimization
@model Academy.Presentation.ViewModels.Profile
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/Internal.cshtml";
}
@section styles {
    @Styles.Render("~/Content/tree")
}
@section UserName
{
    @{ Html.RenderPartial("UserName", Model); }
}
@section Profile
{
    <div class="span3">
        @{ Html.RenderPartial("Profile", Model); }
    </div>
}
<div class="span6">
    @{ Html.RenderPartial("ProfileFieldsEditot", Model); }
</div>
<div class="span3">
    @{ Html.RenderPartial("DisciplinesSelect", Model); }
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/tree")
}
